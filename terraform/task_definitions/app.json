[
  {
    "name": "app",
    "image": "httpd",
    "essential": true,
    "portMappings": [
      {
        "containerPort": 80,
        "hostPort": 80
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-region": "ap-northeast-1",
        "awslogs-group": "${app_log_group_name}",
        "awslogs-stream-prefix": "fargate"
      }
    }
  }
]
